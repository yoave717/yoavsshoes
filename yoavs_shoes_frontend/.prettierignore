# Ignore build output
/dist
/build
/.next
/out

# Ignore node_modules
node_modules

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore package-lock.json (often auto-generated)
package-lock.json
yarn.lock

# Ignore coverage reports
/coverage

# Ignore environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore generated files
*.min.js
*.min.css

# Ignore specific files that shouldn't be formatted
public/static/*
